{
  "image": "swift:5.9.2",
  "customizations": {
    "vscode": {
      "settings": {
        "lldb.adapterType": "bundled",
        "lldb.executable": "/usr/bin/lldb",
        "swift.sourcekit-lsp.serverPath": "/usr/bin/sourcekit-lsp",
        "sde.languageServerMode": "sourcekit-lsp"
      },
      "extensions": ["sswg.swift-lang"]
    }
  },
  "postCreateCommand": "swiftc --version"
}
